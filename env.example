# Firebase Configuration (Server-side only - not exposed to client)
FIREBASE_API_KEY=your_api_key_here
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# API Configuration (Public - used by client to know where to make requests)
EXPO_PUBLIC_API_URL=http://localhost:8081

# Server/Build Configuration (Private - not exposed to client)
NODE_ENV=development

# Private Keys (Never make these public!)
# FIREBASE_ADMIN_PRIVATE_KEY=your_private_key_here
# DATABASE_URL=your_database_url
# JWT_SECRET=your_jwt_secret
